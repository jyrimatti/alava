let

  bool : Type
  bool = (xx : Type) -> xx -> xx -> xx

  true : bool
  true = \x. \y. \z. y
 
  false : bool
  false = \x. \y. \z. z

  not : bool -> bool
  not = \b . (((b bool) false) true)

  Unit : Type
  Unit = (x : Type) -> x

  RR : (x: Type) -> (y: Type) -> bool -> Type
  RR = \x . \y . \b . b x y

  if : (x:Type) -> (y:Type) -> (b: bool) -> x -> y -> RR x y b
  if = \tx . \ty . \b . \x . \y . b x y

  and : Type -> Type -> Type
  and = \p. \q. (c: Type) -> (p -> q -> c) -> c

  T : bool -> Type
  T = \b. if b Type Unit bool

  bar : (b : bool) -> T b
  bar = \b . if b Type Unit true

in 
  bool